Class {
	#name : #TUILibrary,
	#superclass : #WAFileLibrary,
	#category : #'Toast-UI'
}

{ #category : #js }
TUILibrary class >> ajaxInitScript [
	^ 'TuiEditor.initNotLoadedEditor()' js
]

{ #category : #js }
TUILibrary >> bindingJs [
	^ 'class TuiEditor {
    static instancesMap = new WeakMap()
    static extensions = {}
    static chartOptions = {
        minWidth: 100,
        maxWidth: 600,
        minHeight: 100,
        maxHeight: 300
    };

    static codeHighlightOptions = {
        highlighter: Prism
    }

    static initNotLoadedEditor() {
        let editors = Array.from(document.querySelectorAll(`.tui-editable[data-loaded="false"]`))
        editors.forEach(html => {
            new TuiEditor(html)
        })
    }

    static getInstance(querySelector) {
        return this.instancesMap.get(document.querySelector(querySelector))
    }

    static save(selector) {
        TuiEditor.getInstance(selector).save()
    }

    static saveCompleted() {
        document.body.style.cursor = "default"
    }

    static saveFailed() {
        document.body.style.cursor = "default"
    }

    constructor(editor) {
        this.delayMap = new Map()
        this.editor = editor
        let plugins = toastui.Editor.plugin
        let options = {
            relativeContainer: editor,
            el: editor,
            height: ''100%'',
            initialValue: this.editor.innerHTML,
            initialEditType: ''wysiwyg'',
            toolbarItems: [
                [''heading'', ''bold'', ''italic'', ''strike''],
                [''hr'', ''quote''],
                [''ul'', ''ol'', ''task'', ''indent'', ''outdent''],
                [''table'', ''image'', ''link''],
                [''code'', ''codeblock''],
                // Using Option: Customize the last button
                [{
                    el: this.createSaveButton(),
                    command: ''save'',
                    tooltip: ''Save modifications''
                }]
            ],
            plugins: [
                plugins.colorSyntax,
                plugins.tableMergedCell,
                //[plugins.codeSyntaxHighlight, TuiEditor.codeHighlightOptions],
                [plugins.chart, TuiEditor.chartOptions]
            ]
        }
        if(editor.dataset.widgetRules) {
            options.widgetRules = JSON.parse(editor.dataset.widgetRules)
                .map( str => TuiEditor[str])
                .filter( e => e !== undefined)
        }
        this.tuiInstance = new toastui.Editor(options)

        TuiEditor.instancesMap.set(editor, this)
    }

    getValue() {
        return this.tuiInstance.getMarkdown()
    }

    delay(key, duration, fct) {
        if (this.delayMap.has(key)) {
            clearTimeout(this.delayMap.get(key))
        }
        this.delayMap.set(key, setTimeout(() => {
            this.delayMap.delete(key)
            fct()
        }, duration))
    }

    save() {
        document.body.style.cursor = "progress"
        return Function(''"use strict";return ('' + this.editor.dataset["saveCallback"] + '')'')(this)
    }

    createSaveButton() {
        const button = document.createElement(''button'');
        button.className = ''toastui-editor-toolbar-icons save'';
        button.style.backgroundImage = ''none'';
        button.style.margin = ''0'';
        button.innerHTML = `Save`;
        button.addEventListener(''click'', () => {
            this.save()
        });

        return button;
    }
}


window.addEventListener("load", TuiEditor.initNotLoadedEditor)'
]

